{
  "name": "squeezebox",
  "description": "Squeezebox integration for Alexa",
  "region": "eu-west-1",
  "handler": "handler.lambda_handler",
  "role": "arn:aws:iam::917070425775:role/service-role/squeezebox",
  "ignore": [
    "\\.git",
    "\\.idea/",
    "\\.cache/",
    "\\.tox/",
    "tox.ini",
    "metadata/",
    "\\.coverage",
    ".+\\.pyc",
    ".+\\.md",
    ".+\\.zip",
    ".+\\.yml",
    ".+\\.log",
    ".+\\.po",
    "bin/",
    "etc/",
    "docs/",
    "test/",
    "tests/",
    "lib/",
    "easy_install.py",
    "requirements.txt",
    "pip/",
    "pkg_resources/",
    "packaging/",
    "wheel/",
    "setuptools/",
    "pyparsing.py",
    "\\.dist-info/",
    "lambda.json"
  ],
  "timeout": 7,
  "memory": 128
}
