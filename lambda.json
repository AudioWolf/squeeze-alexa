{
  "name": "squeezebox",
  "description": "Squeezebox integration for Alexa",
  "region": "eu-west-1",
  "handler": "handler.lambda_handler",
  "role": "arn:aws:iam::917070425775:role/service-role/squeezebox",
  "ignore": [
    "\\.git",
